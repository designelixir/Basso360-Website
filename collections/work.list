<style>
.vimeo-bg-wrapper {width: 100%; position: absolute; height: 100%; z-index: -1; overflow: hidden; background-size: cover; background-position: center center; background-repeat: no-repeat;}
.vimeo-bg-iframe {width: 1000px; height: 600px; min-height: 600px; min-width: 1000px; position: absolute; transform: translate(-50%, -50%); }
</style>
<!-- item loop -->
{.repeated section items}

  <div style="width: 100%; height: 500px; background-image: url{assetUrl}; border: 2px solid red;">
  {.if oembed}
  <div style="border: 2px solid lime; max-width: 1000px; overflow: hidden;">
    {html}
  </div>

   {.or}
   <h1>{title}</h1>
  {.end}
  </div>



{.end}
